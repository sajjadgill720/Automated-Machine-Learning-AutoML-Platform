# AutoML Frontend Environment Configuration
# Copy this file to .env.local and update values as needed

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_API_TIMEOUT=300000

# App Configuration
VITE_APP_NAME=AutoML Platform
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# File Upload Limits (in MB)
VITE_MAX_FILE_SIZE_MB=500

# Polling Configuration
VITE_JOB_POLL_INTERVAL_MS=2000
VITE_JOB_MAX_ATTEMPTS=120
